<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="Sabina" id="regions-1">
        <createTable tableName="regions">
            <column name="id" type="VARCHAR(6)">
                <constraints primaryKey="true" nullable="false" primaryKeyName="regionsPK"/>
            </column>
            <column name="name_eng" type="VARCHAR(256)"/>
            <column name="name_kaz" type="VARCHAR(256)"/>
            <column name="name_rus" type="VARCHAR(256)"/>
            <column name="region_type" type="VARCHAR(256)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>