<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!--Schema: modify, create, drop, alter command-->
    <include file="/schema/role.xml" relativeToChangelogFile="true"/>
    <include file="/schema/privilege.xml" relativeToChangelogFile="true"/>
    <include file="/schema/role_privilege.xml" relativeToChangelogFile="true"/>
    <include file="/schema/organization.xml" relativeToChangelogFile="true"/>
    <include file="/schema/user.xml" relativeToChangelogFile="true"/>
    <include file="/schema/token.xml" relativeToChangelogFile="true"/>

    <!--Data: insert, update, delete-->
    <include file="/data/roles.sql" relativeToChangelogFile="true"/>
    <include file="/data/privileges.sql" relativeToChangelogFile="true"/>
    <include file="/data/roles_privileges.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>